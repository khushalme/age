//program to check if you are eligble to vote or not!
#include<iostream>
using namespace std;
int main()
{
int n;
cout<<"Enter your age"<<endl;
cin>>n;
cout<<"Minimum age to vote is 18!"<<endl;
if(n>=18)
{
	cout<<"You are eligble to vote"<<endl;
}
else
{
	cout<<"You are not eligble to vote "<<endl;
}
return 0;
}
